<%# This is used to deliver a static ad for the single-touch attribution use-case.
    This uses the attributionsrc HTML attribute implementation pattern.
#%>
<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport"
        content="width=device-width, initial-scale=1.0" />
  <title><%= TITLE %></title>
  <link rel="icon"
        href="" />
  <link rel="stylesheet"
        href="/css/ad-frame.css" />
  <link rel="stylesheet"
        href="/css/ad-label.css" />
  <script src="/js/dsp/static-ad-label.js"></script>
</head>

<body>
  <p id="ad-label"></p>
  <a width="300"
     height="250"
     target="_blank"
     rel="noopener noreferrer"
     attributionsrc="<%= ATTRIBUTION_SRC %>"
     href="<%= DESTINATION %>">
    <!-- Smaller to avoid scrollbar -->
    <img width="294"
         height="245"
         loading="lazy"
         attributionsrc="<%= ATTRIBUTION_SRC %>"
         src="<%= CREATIVE %>">
  </a>
</body>

</html>
